# Sample environment variables for act local testing
# Copy this file to .env and fill in your actual values

# Firebase Cloud Messaging (for integration tests)
FIREBASE_CREDENTIALS_JSON={"type":"service_account","project_id":"your-project","private_key_id":"your-key-id"}

# GitHub Token (for dorny/test-reporter action)
GITHUB_TOKEN=your_github_token

# Railway deployment (optional for local testing)
PORT=8080